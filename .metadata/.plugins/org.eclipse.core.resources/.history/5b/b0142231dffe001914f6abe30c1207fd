
public class HolaMundo {

	public static void main(String[] args) { //esta función es el punto de acceso
		//Esto es un comentario de una línea
		/*
		 * Esto sería 
		 * un comentario
		 * de varías líneas
		 */
		 System.out.println("Hola Mundo");
		 System.out.println("Otra vez Hola Mundo");

	}

}
