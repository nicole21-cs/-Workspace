
public class EjercicioConFuncion {

	public static void main(String[] args) {
		Coche c1 = new Coche();
		c1.matricula = "1111ADD";
		c1.marca = "Mercedes";
		c1.modelo = "Mercedes C-4";
		c1.mantenimiento = "Nuevo";
		imprimirCoche(c1.matricula, c1.marca, c1.modelo, c1.mantenimiento);
		
		Coche c2 = new Coche();
		c2.matricula = "8508DXX";
		c2.marca = "Opel";
		c2.modelo = "Zafira 96";
		c2.mantenimiento = "Segunda mano";
		imprimirCoche(c2.matricula, c2.marca, c2.modelo, c2.mantenimiento);
		
		Coche c3 = new Coche();
		c3.matricula = "6754KFC";
		c3.marca = "Seat";
		c3.modelo = "León";
		c3.mantenimiento = "Segunda mano";
		imprimirCoche(c3.matricula,c3.marca, c3.modelo, c3.mantenimiento);

	}
     public static void imprimirCoche(String matricula , String marca , String modelo , String mantenimiento ) {
    	 System.out.println("----COCHE----");
    	 System.out.println(matricula);
    	 System.out.println(marca);
    	 System.out.println(modelo);
    	 System.out.println(mantenimiento);
    	 
     }
    
}
