
public class MainMoto {

	public static void main(String[] args) {
		Moto moto = new Moto("1234", "Yahama", 0 , 20);
		int posicion = moto.acelerar(true);
		System.out.println(posicion);
		moto.acelerar(true);
		moto.acelerar(true);
		moto.acelerar(true);
		
		System.out.println(moto);
		moto.acelerar(false);

	}

}
